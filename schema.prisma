generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id
  presets  Json
  premium  String?
  timezone String?
}

model Event {
  id          String @id
  channelId   String
  userId      String
  type        String
  ping        String?
  endTime     BigInt
  timeString  String? // timers only
  title       String? // reminders only
  desc        String? // reminders only
}